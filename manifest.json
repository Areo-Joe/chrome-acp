{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "sidePanel"
  ],
  "action": {
    "default_title": "Open Side Panel"
  },
  "side_panel": {
    "default_path": "dist/sidepanel.html"
  },
  "background": {
    "service_worker": "dist/background.js",
    "type": "module"
  }
}
